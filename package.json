{
  "name": "temi-construction-crm",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "fix:default-exports": "node scripts/fix-default-exports.js",
    "fix:pages-default": "node scripts/ensure-default-exports.js",
    "fix:react-imports": "node scripts/clean-react-duplicates.cjs",
    "fix:react-merge": "node scripts/merge-react-imports.js",
    "fix:lucide-imports": "node scripts/fix-lucide-imports.js",
    "build": "npm run fix:lucide-imports && vite build",
    "build:verbose": "cross-env DEBUG=* VITE_DEBUG=1 vite build --debug --logLevel info",
    "build:capture": "node scripts/run-build-and-capture.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "generate:page": "node scripts/generate-page.js",
    "generate:component": "node scripts/generate-component.js",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint . --max-warnings 0",
    "check:format": "prettier --check .",
    "check:lazy": "tsx tools/check-lazy-routes.ts",
    "scan:routes": "tsx tools/scan-hardcoded-routes.ts",
    "prebuild": "npm run check:types && npm run check:lazy",
    "type-check": "npm run check:types",
    "format": "prettier --write .",
    "test:smoke": "playwright test --grep smoke",
    "test:routes": "playwright test routes.spec.ts",
    "validate": "npm run check:types && npm run check:lint && npm run test:routes",
    "dev:full": "npm run validate && npm run dev",
    "build:ci": "npm run validate && npm run build",
    "setup": "npm install && npx playwright install",
    "clean": "rm -rf dist node_modules/.vite",
    "reset": "npm run clean && npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.12.0",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.540.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.4",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.22.3",
    "react-signature-canvas": "^1.0.6",
    "react-webcam": "^7.2.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.40.0",
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-signature-canvas": "^1.0.5",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.9"
  }
}
